{
    "name": "jsunicode",
    "version": "1.2.4",
    "description": "A JavaScript library for encoding/decoding unicode",
    "main": "src/jsunicode.js",
    "watch": {
        "lint_test": "{src,test}/*.js"
    },
    "scripts": {
        "build": "webpack --progress --env=dev",
        "start": "webpack --progress --env=dev && wds",
        "test": "node test/jsunicode.test.js",
        "lint": "eslint src test",
        "lint_test": "npm run lint && npm run test",
        "watch-server": "nodemon -i dist --exec \"npm start\"",
        "watch": "nodemon --exec \"npm run lint_test\""
    },
    "keywords": [
        "JavaScript",
        "unicode"
    ],
    "author": "Jeremy Rann",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/JeremyRann/JSUnicode.git"
    },
    "devDependencies": {
        "@web/dev-server": "^0.3.1",
        "detect-node": "^2.0.4",
        "eslint": "^7.2.0",
        "eslint-loader": "^4.0.2",
        "nodemon": "^2.0.4",
        "tap-spec": "^5.0.0",
        "tape": "^5.0.1",
        "tape-catch": "^1.0.6",
        "tape-worm": "^0.0.3",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.11",
        "webpack-dev-server": "^3.11.0"
    }
}
